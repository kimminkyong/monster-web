<!DOCTYPE html>
<html lang="ko">
<head>
<meta charset="utf-8">
<title><%= title %></title>
<link rel="stylesheet" href="/stylesheets/ui.admin.css"> 
<link rel="stylesheet" href="/stylesheets/circle.css"> 

</head> 


<body> 
<div class="wrap"> 
    <div class="container"> 
        
        <% include ./inc/header %>
        <section>
            <div class="header">
                <div class="header-title">
                    Header Info
                </div>
                <div class="header-info">
                    Info
                </div>
                <dlv class="login">
                    Login Info
                </dlv>
            </div>
            <div class="contents">
                <h3>Dashboard</h3> 

                <div class="box"> 
                    <div class="dan">
                        <h4>TOTAL ITEM</h4>
                        <div class="c100 p100 center">
                            <span><%= total_item_length %></span>
                            <div class="slice">
                                <div class="bar"></div>
                                <div class="fill"></div>
                            </div>
                        </div>
                    </div> 
                    <div class="dan">
                        <h4>STEP 1</h4>
                        <div class="c100 p54 green center">
                            <span><%= step1_length %></span>
                            <div class="slice">
                                <div class="bar"></div>
                                <div class="fill"></div>
                            </div>
                        </div>
                    </div> 
                    <div class="dan">
                        <h4>STEP 2</h4>
                        <div class="c100 p72 orange center">
                            <span><%= step2_length %></span>
                            <div class="slice">
                                <div class="bar"></div>
                                <div class="fill"></div>
                            </div>
                        </div>
                    </div> 
                </div> 

                
                <div class="box"> 
                    <div class="dan"> 
                        <h4><%= listDate %> (<%= rowList.length %>)</h4> 
                        
                        <div class="box-contents"> 
                            <ul class="list"> 
                              <% rowList.forEach( (item) => { %>
                                <li>
                                  <a href=''> 
                                    <strong><%= item.name %> [<%= item.code %>]</strong> - <span>(<%= item.alg_step %>)</span>
                                  </a>
                                </li>
                              <% }); %>
                            </ul> 
                        </div>
                    </div> 
                    <div class="dan"> 
                        <h4>Box Title</h4> 
                        <div class="box-contents"> 
                            <ul class="list"> 
                            <li>List 1</li> 
                            <li>List 2</li> 
                            <li>List 3</li> 
                            <li>List 4</li> 
                            </ul> 
                        </div> 
                    </div> 
                </div> 
                
                <h3>Contents</h3> 
                <div class="box"> 
                    <div class="dan">1</div> 
                    <div class="dan">2</div> 
                    <div class="dan">3</div> 
                    <div class="dan">4</div> 
                </div> 
                <div class="box"> 
                    <div class="dan">1</div> 
                    <div class="dan">2</div> 
                    <div class="dan">3</div> 
                    <div class="dan">4</div> 
                    <div class="dan">5</div> 
                </div>
            </div>
        </section> 
    </div> 
</div> 
</body> 
</html> 